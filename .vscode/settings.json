{
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/css"
    }
  ],
  "liveSassCompile.settings.excludeList": [
    "**/node_modules/**",
    "./vscode/**",
    "**/build/**",
    "**/documentation/**",
    "**/plugins/**",
    "**/fonts/**"
  ],
  "html.suggest.html5": true,
  "emmet.includeLanguages": {
    "njk": "html",
    "nunjucks": "html"
  },
  "liveServer.settings.port": 5511,
  "cSpell.words": ["Furniq", "Furniqe"]
}
